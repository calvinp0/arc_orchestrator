{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "frontend",
  "version": "0.1.0",
  "identifier": "com.calvin.frontend",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "frontend",
        "width": 1580,
        "height": 1200,
        "minWidth": 960,
        "minHeight": 600,
        "center": true,
        "resizable": true
      }
    ],
    "security": {
      "csp": null,
      "capabilities": ["allow-store", "allow-opener", "allow-dialog", "allow-shell", "allow-dialog-fs", "default"]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
